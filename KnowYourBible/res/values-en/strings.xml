	<string name="aboutgame">Know Your Bible (KYB) is a fun and educational game to help anyone learn and memorize Bible scriptures.\n
	    \n
	    KYB was created as a team course project in CS378 Mobile Computing at The University of Texas at Austin during the Fall 2013 semester.\n
	    \n
	    Date Created: December 15, 2013\n
	    Created by: Carlos Hernandez, Khuong Thai, and Hoang Pham\n
	    </string>
	<string name="helpgame">1) To start the game click on the KYB icon on your home screen or application list.\n
	    	2) The Welcome Screen will display for a few seconds to setup the scriptures database and installation file.\n
	    	3) After the Welcome Screen, the Main Menu screen will display three buttons: I Know That Scripture, Scriptures Database, and More Games.\n
	    	 
	    		</string>
	
	<string name="passage_a1">Keep your senses, be watchful! Your adversary, the Devil, walks about like a roaring lion, seeking to devour someone.</string>
	<string name="passage_b1">@string/book60</string>
	<string name="passage_c1">5</string>
	<string name="passage_d1">8</string>
	
	<string name="passage_a2">Happy are the pure in heart, since they will see God.</string>
	<string name="passage_b2">@string/book40</string>
	<string name="passage_c2">5</string>
	<string name="passage_d2">8</string>
	
	<string name="passage_a3">In the beginning God created the heavens and the earth.</string>
	<string name="passage_b3">@string/book1</string>
	<string name="passage_c3">1</string>
	<string name="passage_d3">1</string>
	
	<string name="passage_a4">Serve Jehovah with rejoicing. Come in before him with a joyful cry.</string>
