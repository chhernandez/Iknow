	<string name="passage_c4">100</string>
	<string name="passage_d4">2</string>
	
	<string name="passage_a5">As the last enemy, death is to be brought to nothing.</string>
	<string name="passage_b5">@string/book46</string>
	<string name="passage_c5">15</string>
	<string name="passage_d5">26</string>
	
	<string name="passage_a6">Always rejoice in the Lord. Once more I will say, Rejoice!</string>
	<string name="passage_b6">@string/book50</string>
	<string name="passage_c6">4</string>
	<string name="passage_d6">4</string>
	
	<string name="passage_a7">Not many of you should become teachers, my brothers, knowing that we shall receive heavier judgment.</string>
	<string name="passage_b7">@string/book59</string>
	<string name="passage_c7">3</string>
	<string name="passage_d7">1</string>
	
	<string name="passage_a8">You were bought with a price; stop becoming slaves of men.</string>
	<string name="passage_b8">@string/book46</string>
	<string name="passage_c8">7</string>
	<string name="passage_d8">23</string>		
	
	<string name="passage_a9">So, then, let us not sleep on as to rest do, but let us stay awake and keep our senses.</string>
	<string name="passage_b9">@string/book52</string>
	<string name="passage_c9">5</string>
	<string name="passage_d9">6</string>
	
	<string name="passage_a10">Even those troubled with unclean spirits were cured.</string>
	<string name="passage_b10">@string/book42</string>
	<string name="passage_c10">6</string>
	<string name="passage_d10">18</string>
	
	<string name="passage_a11">Well, then, have I become YOUR enemy because I tell YOU the truth?</string>
	<string name="passage_b11">@string/book48</string>
	<string name="passage_c11">4</string>
	<string name="passage_d11">16</string>
	
	<string name="passage_a12">And the family heads became jealous of Joseph and sold him into Egypt.</string>
	<string name="passage_b12">@string/book44</string>
	<string name="passage_c12">7</string>
	<string name="passage_d12">9</string>
	
	<string name="passage_a13">The tree that you saw that grew great and became strong, whose top reached the heavens and was visible to all the earth,</string>
	<string name="passage_b13">@string/book27</string>
	<string name="passage_c13">4</string>
	<string name="passage_d13">20</string>	
	
	<string name="passage_a14">Teach me to do your will, for you are my God. Your spirit is good; may it lead me on level ground.</string>
